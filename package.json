{
  "name": "remark-preset-gfm",
  "version": "1.0.0",
  "main": "index.js",
  "description": "Remark preset for gfm",
  "author": "imcuttle <moyuyc95@gmail.com>",
  "scripts": {
    "test": "jest",
    "prepare": "node gen-link-content.js",
    "prepublishOnly": "npm test",
    "version": "npm run changelog",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=6"
  },
  "files": [
    "link-content.json",
    "index.js"
  ],
  "keywords": [
    "imcuttle",
    "remark",
    "preset",
    "gfm",
    "remark-preset-gfm"
  ],
  "license": "MIT",
  "repository": "imcuttle/remark-preset-gfm",
  "jest": {
    "testMatch": [
      "**/__test{s,}__/*.(spec|test).{t,j}s{x,}"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^23.3.7",
    "conventional-changelog-cli": "^2.0.5",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "rehype-parse": "^5.0.0",
    "rehype-raw": "^3.0.0",
    "rehype-stringify": "^4.0.0",
    "remark": "^10.0.0",
    "remark-frontmatter": "^1.3.0",
    "remark-gemoji-to-emoji": "^1.1.0",
    "remark-github-break": "0.0.0",
    "remark-highlight.js": "^5.0.0",
    "remark-html": "^8.0.0",
    "remark-rehype": "^3.0.1",
    "remark-slug": "^5.1.0",
    "unified": "^7.0.0"
  },
  "dependencies": {
    "@rigor789/remark-autolink-headings": "^5.1.0"
  }
}
